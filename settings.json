{
  "$comment": "プラグイン",
  "$comment": "vscode-neovim",
  "$comment": "nvimのpath",
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\yuu_harada\\scoop\\apps\\neovim\\current\\bin\\nvim.exe",
  "tabnine.experimentalAutoImports": true,
  "$comment": "カラースキーム",
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "$comment": "CompareFolders",
  "$comment": "空行と空白を無視する",
  "compareFolders.ignoreEmptyLines": true,
  "compareFolders.ignoreAllWhiteSpaces": true,
  "compareFolders.ignoreWhiteSpaces": true,
  "$comment": "env",
  "files.associations": {
    "*.env.development*": "env",
    "*.env.staging*": "env",
    "*.env.production*": "env"
  },
  "$comment": "AWS",
  "aws.suppressPrompts": {
    "yamlExtPrompt": true
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": false,
  "$comment": "フォーマッター",
  "[javascript, html, css, typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "$comment": "Go-lang",
  "go.formatTool": "gofmt",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "$comment": "デフォルト",
  "$comment": "ズームレベル",
  "window.zoomLevel": 0.68,
  "$comment": "起動時に前回開いたウインドウを開かない",
  "window.restoreWindows": "none",
  "$comment": "自動保存",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "editor.formatOnSave": true,
  "$comment": "同期する設定",
  "settingsSync.ignoredSettings": ["-window.zoomLevel"],
  "$comment": "スタート画面",
  "workbench.startupEditor": "none",
  "$comment": "ファイル末尾に改行を入れる",
  "files.insertFinalNewline": true,
  "$comment": "行末の空白自動削除",
  "files.trimTrailingWhitespace": true,
  "$comment": "アイコン",
  "workbench.iconTheme": "vscode-icons",
  "$comment": "ツールチップ表示",
  "editor.hover.enabled": false,
  "$comment": "ユーザー辞書",
  "cSpell.userWords": [
    "asseinfo",
    "kanban",
    "Tauri"
  ],
  "$comment": "ここまでは手動設定",
}
